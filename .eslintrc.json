{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "no-console": "off",
      "object-curly-newline": ["error", { "multiline": true }],
      "consistent-return": "off",
      "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }]
    }
}
