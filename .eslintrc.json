{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "import/extensions": "off",
        "import/no-unresolved": "off",
        "no-console": "off",
        "object-curly-newline": ["error", { "multiline": true }],
        "consistent-return": "off",
        "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }]
      }
}
